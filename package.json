{
  "name": "unotifier",
  "version": "0.0.1",
  "description": "Universal push notifications for free using a telegram bot and a REST API.",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "dev": "tsc --watch & nodemon --watch build --delay 1 build/index.js",
    "build": "tsc",
    "typescript:watch": "tsc --watch",
    "dev:nodemon": "nodemon --watch build build/index.js",
    "lint": "eslint src --ext .ts",
    "prisma:studio": "prisma studio",
    "prisma:introspect": "prisma introspect",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate-reset": "prisma migrate reset",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pjnicolas/unotifier.git"
  },
  "keywords": [
    "unotifier",
    "telegram",
    "bot",
    "api",
    "rest"
  ],
  "author": "pjnicolas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pjnicolas/unotifier/issues"
  },
  "homepage": "https://github.com/pjnicolas/unotifier#readme",
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "nodemon": "^2.0.7",
    "prisma": "^2.23.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "prisma": "^2.23.0",
    "@prisma/client": "^2.23.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "telegraf": "^4.3.0"
  }
}
